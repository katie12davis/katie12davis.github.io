{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "IMF Lending Outstanding ",
    "subtitle": "Total IMF Lending Outstanding for all members from 1984-2022 in USD (as of 05/10/2022). Source: IMF",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/katie12davis/katie12davis.github.io/main/IMF%20outstanding%20lending%2012102022.csv"
  },
  "height": 300,
  "width": 300,
  "mark": {"type": "bar", "color": "RGB(120,200,200)"},
  "encoding": {
    "x": {
      "field": "Year ",
      "type": "temporal",
      "axis": {"title": "Year", "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "Total_Billions_USD",
      "type": "quantitative",
      "title": "Total lending ($/billions)",
      "axis": {"grid": false}
    }
  },
  "config": {}
}